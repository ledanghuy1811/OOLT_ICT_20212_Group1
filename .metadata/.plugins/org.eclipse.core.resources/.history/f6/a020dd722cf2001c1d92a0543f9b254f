package algorithms;

import graph.*;

public abstract class Algorithms {
	//attributes
	private Graph graph = new Graph();
	private int numVertex;
	
	//constructor
	public Algorithms() {
		
	}
	public Algorithms(Graph graph, int numVertex) {
		this.graph = graph;
		this.numVertex = numVertex;
	}
	
	//getter and setter
	public int getNumVertex() {
		return numVertex;
	}
	public Graph getGraph() {
		return graph;
	}
	
	
	//to override
	public abstract void execute();
}
