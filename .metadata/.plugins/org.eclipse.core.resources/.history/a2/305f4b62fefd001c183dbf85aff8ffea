package step;

import java.util.ArrayList;

public class StepPseudo {
	private ArrayList<String> pseudo = new ArrayList<String>(); 
	
	public StepPseudo() {
		
	}
	public StepPseudo(ArrayList<String> pseudo) {
		this.pseudo = pseudo;
	}
	
	public ArrayList<String> getPseudo() {
		return this.pseudo;
	}
	public void setPseudo(ArrayList<String> pseudo) {
		this.pseudo = pseudo;
	}
	
	public void printPseudo(int index) {
		System.out.println(this.getPseudo().get(index));
	}
}
